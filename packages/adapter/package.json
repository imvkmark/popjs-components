{
  "name": "@wakeadmin/element-adapter",
  "version": "0.2.5",
  "description": "基础组件适配层",
  "sideEffects": false,
  "exports": "./lib/index.js",
  "module": "./lib/index.js",
  "main": "./lib/index.cjs",
  "typings": "./types/index.d.ts",
  "engines": {
    "node": ">= 16"
  },
  "bin": {
    "wakeadmin-adapter-switch": "bin/wakeadmin-adapter-switch.mjs"
  },
  "files": [
    "lib",
    "bin",
    "src",
    "dist",
    "types",
    "scripts"
  ],
  "scripts": {
    "prebuild": "rimraf dist .tsBuildInfo",
    "build": "npm run build:es && npm run build:common",
    "build:es": "tsc --build tsconfig.json",
    "build:common": "tsc --build tsconfig.common.json",
    "start": "tsc --build tsconfig.json --watch",
    "postinstall": "node ./scripts/postinstall.mjs",
    "test": "jest"
  },
  "keywords": [],
  "author": "ivan-lee",
  "license": "MIT",
  "dependencies": {
    "@wakeadmin/demi": "^0.1.5",
    "@wakeadmin/h": "^0.2.8",
    "@wakeadmin/utils": "^0.1.2"
  },
  "devDependencies": {
    "element-plus": "^2.2.15",
    "element-ui": "^2.15.9"
  },
  "peerDependencies": {
    "element-plus": "^2.2.12",
    "element-ui": "^2.15.9"
  },
  "peerDependenciesMeta": {
    "element-ui": {
      "optional": true
    },
    "element-plus": {
      "optional": true
    }
  }
}
