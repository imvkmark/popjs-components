{
  "name": "@wakeadmin/components",
  "version": "0.1.0",
  "description": "wakeadmin 组件库",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -b tsconfig.build.json --watch",
    "prebuild": "rimraf dist .tsBuildInfo",
    "copy": "cpx \"src/**/*.+(scss|jpg|jpeg|gif|png|svg|css)\" ./dist",
    "test": "vitest",
    "build": "tsc -b tsconfig.build.json",
    "postbuild": "npm run copy",
    "type-check": "tsc"
  },
  "keywords": [
    "wakeadmin",
    "components"
  ],
  "author": "ivan-lee",
  "license": "MIT",
  "dependencies": {
    "@wakeadmin/component-adapter": "workspace:^1.0.0",
    "@wakeadmin/demi": "latest",
    "@wakeadmin/h": "^0.2.4",
    "@wakeadmin/icons": "latest",
    "@wakeadmin/utils": "latest"
  },
  "devDependencies": {
    "vitest": "^0.23.1"
  }
}
