{
  "name": "@wakeadmin/components-repo",
  "version": "0.1.0",
  "description": "wakeadmin components",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "local-check": "wkstd local-check",
    "switch:3": "vue-demi-switch 3  && wakeadmin-adapter-switch 3",
    "switch:2": "vue-demi-switch 2.7 vue2 && wakeadmin-adapter-switch 2",
    "update:wakeadmin": "pnpm up -r -L \"@wakeadmin/*\"",
    "prepare": "husky install",
    "build": "pnpm run -r --filter=!playground-* build",
    "test": "pnpm run -r --filter=!playground-* --workspace-concurrency=1 test --passWithNoTests --silent",
    "type-check": "pnpm run -r type-check"
  },
  "author": "ivan-lee",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "~7.18.13",
    "@changesets/cli": "^2.24.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/vue": "^6.6.1",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^17.0.45",
    "@types/tapable": "^2.2.2",
    "@vue/test-utils": "next",
    "@wakeadmin/component-adapter": "workspace:^1.0.0",
    "@wakeadmin/docker-build": "^0.1.8",
    "babel-jest": "^28.1.3",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "eslint-config-wkts": "^0.2.2",
    "eslint-config-wkvue": "^0.2.2",
    "eslint-plugin-jest": "^26.9.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-config-wk": "^0.2.2",
    "rimraf": "^3.0.2",
    "testing-library-vue-2": "npm:@testing-library/vue@^5.8.3",
    "tsd": "^0.21.0",
    "typescript": "^4.8.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.5",
    "vue-router-3": "npm:vue-router@^3.6.4",
    "vue-template-compiler": "^2.7.10",
    "vue-test-utils-2": "npm:@vue/test-utils@^1.3.0",
    "vue2": "npm:vue@^2.7.10",
    "whatwg-fetch": "^3.6.2",
    "wkstd": "^0.2.2"
  },
  "prettier": "prettier-config-wk"
}
